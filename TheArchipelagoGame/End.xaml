<Page x:Class="TheArchipelagoGame.End"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TheArchipelagoGame"
      mc:Ignorable="d" 
      d:DesignHeight="530" d:DesignWidth="800"
      Title="End">

    <Grid>
        <Rectangle Margin="139,196,139,0" Stroke="Black" Fill="#FFF6EAE0" Height="163" VerticalAlignment="Top"/>
        <Label x:Name="IslandTitle" Content="TITLE" Margin="61,23,152,0" Foreground="#FFF6EAE0" FontFamily="Showcard Gothic" FontSize="36" Height="153" VerticalAlignment="Top">
            <Label.Effect>
                <DropShadowEffect/>
            </Label.Effect>
        </Label>
        <TextBlock x:Name="IslandDescription" Margin="0,211,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="124" Foreground="#FF1E2020" FontFamily="Rockwell" FontSize="16" HorizontalAlignment="Center" Width="472"/>
        <Button x:Name="Button3" Content="Exit" Margin="586,425,0,0" VerticalAlignment="Top" Height="58" FontSize="36" FontFamily="Rockwell" Click="NavigateButton_Click" Foreground="#FF1E2020" Background="#FFF6EAE0" HorizontalAlignment="Left" Width="176"/>

    </Grid>
</Page>
